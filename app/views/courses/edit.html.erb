<h1>Edit</h1>
<%= form_for(@course) do |f| %>
  <strong><%= f.label :subject %>:</strong>
  <%= f.text_field :subject %>
  <br>
  <br>
  <strong><%= f.label :readme %>:</strong>
  <%= f.text_area :readme %><br>
  <br>
  <%= f.submit %>
  <%= button_to "Delete Course", course_path(@course.id), method: :delete %>
<% end %>
